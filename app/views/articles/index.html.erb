<h1>Listing Articles</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
    </tr>
  </thead>

  <tbody>
    <% @articles.each do |article| %>
      <tr>
        <td>  
          <%= link_to article.title, article %>
        </td>
        <td><%= article.body %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "New Article", new_article_path %>